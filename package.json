{
  "name": "offline",
  "version": "1.0.0",
  "description": "App that keeps track of time spent offline.",
  "main": "www/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && electron-forge start",
    "package": "tsc && electron-forge package && mv www/out out",
    "release": "tsc && electron-forge package && ls && ls ./www"
  },
  "author": "bramtechs",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "electron": "^24.1.3",
    "typescript": "^5.0.4"
  },
  "prettier": {
    "printWidth": 75,
    "tabWidth": 4
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
